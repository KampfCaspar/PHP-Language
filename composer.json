{
  "name": "kampfcaspar/language",
  "description": "RFC5646 language tag class",
  "homepage": "https://github.com/KampfCaspar/PHP-Language/",
  "license": "AGPL-3.0-or-later",
  "require": {
    "php": "^8.2",
    "ext-ctype": "*",
    "psr/log": "^3.0",
    "kampfcaspar/polyfill": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.10",
    "phpstan/extension-installer": "^1.3",
    "phpstan/phpstan-phpunit": "^1.3",
    "struggle-for-php/sfp-phpstan-psr-log": "^0.17"
  },
  "autoload": {
    "psr-4": {
      "KampfCaspar\\Language\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KampfCaspar\\Test\\Language\\": "tests/"
    }
  },
  "scripts": {
    "import-registry-file": "KampfCaspar\\Language\\Command\\ImportRegistryFileCommand"
  },
  "scripts-descriptions": {
    "import-registry-file": "import IANA subtag registry for StaticRegistry"
  },
  "authors": [
    {
      "name": "KampfCaspar",
      "email": "code@kampfcaspar.ch",
      "role": "Developer"
    }
  ],
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}